{
  "name": "corevia-ia-service",
  "version": "1.0.0",
  "description": "Microservice IA pour Corevia avec RAG et agents spécialisés",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint -c eslint.config.mts src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "nestjs",
    "socket.io",
    "mistral-ai",
    "pinecone",
    "rag"
  ],
  "author": "Corevia",
  "license": "MIT",
  "dependencies": {
    "@mistralai/mistralai": "^0.1.3",
    "@nestjs/common": "^11.1.9",
    "@nestjs/core": "^11.1.9",
    "@nestjs/microservices": "^11.1.9",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/platform-socket.io": "^11.1.9",
    "@nestjs/websockets": "^11.1.9",
    "@pinecone-database/pinecone": "^2.2.2",
    "dotenv": "^16.3.1",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "socket.io": "^4.6.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.14",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.57.1",
    "globals": "^16.5.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.48.1"
  }
}
